{
  "name": "chaiblogsite",
  "version": "1.0.0",
  "description": "My personal static website (blog + portfolio + whatever else)",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node index-blog-posts.js && webpack-dev-server --config ./webpack-dev.config.js",
    "build": "node index-blog-posts.js && webpack --config ./webpack-prod.config.js",
    "upload": "node upload-to-s3.js"
  },
  "keywords": [
    "react",
    "personal",
    "site",
    "blog",
    "portfolio"
  ],
  "author": "Chaitanya Palaka",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^9.1.1",
    "aws-sdk": "^2.509.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^1.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "loader-utils": "^1.2.3",
    "markdown-loader": "^5.1.0",
    "marked": "^0.6.3",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.9.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "fromentries": "^1.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.11",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "reactSnap" : {
    "source": "dist"
  }
}
